<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.vegetables.dao.CommodityCodeMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.vegetables.model.CommodityCode">
        <result column="id" property="id" />
        <result column="create_people" property="createPeople" />
        <result column="create_time" property="createTime" />
        <result column="modify_people" property="modifyPeople" />
        <result column="deleted" property="deleted" />
        <result column="code" property="code" />
        <result column="name" property="name" />
        <result column="type" property="type" />
        <result column="unit_price" property="unitPrice" />
        <result column="unit" property="unit" />
        <result column="modifyTime" property="modifytime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        create_people,
        create_time,
        modify_people,
        deleted,
        code, name, type, unit_price, unit, modifyTime
    </sql>

</mapper>
